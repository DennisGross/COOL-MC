name: openai_gym_training
entry_points:
    main:
        parameters:
            open_ai_env : {type: string, default: "FrozenLake-v0"}
            prism_dir : {type: string, default: "../prism_files"}
            prism_file_path : {type: string, default: "frozen_lake-v1.prism" }
            constant_definitions : {type: string, default: "slippery=0.04" }
            


        command: >-
            python analyze_states.py --open_ai_env={open_ai_env} --prism_dir={prism_dir} --prism_file_path={prism_file_path} --constant_definitions={constant_definitions}