name: openai_gym_training
entry_points:
    main:
        parameters:
            open_ai_env : {type: string, default: "avoid:avoid-v0"}
            prism_dir : {type: string, default: "../prism_files"}
            prism_file_path : {type: string, default: "avoid.prism" }
            constant_definitions : {type: string, default: "" }
            disable_features: {type: tring, default: "done"}


        command: >-
            python analyze_states.py --open_ai_env={open_ai_env} --prism_dir={prism_dir} --prism_file_path={prism_file_path} --constant_definitions={constant_definitions} --disable_features={disable_features}