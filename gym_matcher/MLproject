name: openai_gym_training
entry_points:
    main:
        parameters:
            prism_dir : {type: string, default: "../prism_files"}
            prism_file_path : {type: string, default: "transporter.prism"}
            constant_definitions : {type: string, default: ""}
            env : {type: string, default: ""}

        command: >-
            python run.py --prism_dir={prism_dir} --prism_file_path={prism_file_path} --env={env} --constant_definitions={constant_definitions}