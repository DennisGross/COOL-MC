name: decision_tree_interpretation
entry_points:
    main:
        parameters:
            project_dir : {type: string, default: "../projects"}
            project_name : {type: string, default: "Frozen Lake"}
            parent_run_id : {type: string, default: "dt9ghvnryt3ujlpnjoi80kn779sv5lqyzeq08ttxqw"}
            task: {type: string, default: "decision_tree_training"}
            prism_dir : {type: string, default: "../prism_files"}
            prism_file_path : {type: string, default: "frozen_lake_nxn.prism"}
            constant_definitions: {type: string, default: "slippery=0.04,SIZE=4"}
            
            

        command: >-
            python run.py --project_dir={project_dir} --project_name={project_name} --parent_run_id={parent_run_id} --task={task} --prism_dir={prism_dir} --prism_file_path={prism_file_path} --constant_definitions={constant_definitions}
