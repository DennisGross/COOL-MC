name: unit_testing

entry_points:
  main:
    command: >-
            python run.py
  test_safe_gym:
    command: >-
            pytest -s -vv test_safe_gym.py

  test_safe_gym_features:
    command: >-
            pytest -s -vv test_safe_gym_features.py
  
  test_permissive_gym:
    command: >-
            pytest -s -vv test_permissive_gym.py

  test_whole_pipeline:
    command: >-
            pytest -s -vv test_whole_pipeline.py
            
  test_experiments:
    command: >-
            pytest -s -vv test_experiments.py